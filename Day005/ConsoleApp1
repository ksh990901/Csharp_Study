namespace ConsoleApp1
{
    internal class Program
    {
        static void Main(string[] args)
        {
            int[,] arr = new int[6, 1];
            for (int i = 0; i < arr.GetLength(0); i++) 
            {
                arr[i, 0] = i;
            }
            Console.WriteLine(arr[6,1]);   

        }
    }
}

namespace CalculatorApp01
{
    //사칙연산을 하는 계산기를 만들어 봅시다.
    class Calculator
    {
        // 1.멤버변수
        private int x;
        // 2. 생성자
              // ~ 디폴트 생성자는 안만들면 자동으로 만들어진다.
              // ~ 단 인자가 있는 생성자가 1개라도 있으면 자동을 만들어지지 않는다.
        public Calculator() 
        {
        }
        public Calculator(int _x)
        {
            this.x = _x;
        }
        // 3. 멤버 메소드
    }
    internal class Program
    {
        static void Main(string[] args)
        {
            Calculator cal1 = new Calculator();  // 객체생성
            Calculator cal2 = new Calculator(100);
        }
    }
}

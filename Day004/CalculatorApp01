namespace CalculatorApp01
{
    class Calculator
    {
        //멤버변수
        //생성자
        //멤버메소드
       public int Plus(int a, int b)
        {
            return a + b;
        }
    }
    internal class Program
    {
        static void Main(string[] args)
        {
            Calculator calculator = new Calculator();
            int result = calculator.Plus(100, 200);
            Console.WriteLine(result);

        }
    }
}

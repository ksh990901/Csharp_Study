namespace ArrayApp07
{
    internal class Program
    {
        static void Main(string[] args)
        {
            int[,] array = new int[2, 2]
            {
                { 1, 2 },
                { 3, 4 }
            };

            for(int i = 0 ; i < 2; i++) 
            {
                for(int j=0;  j < 2; j++)
                {
                    Console.Write(array[i, j]+"\t");
                }
                Console.WriteLine();
            }
        }
    }
}

namespace ArratAppQuiz04
{
    internal class Program
    {
        static void Main(string[] args)
        {
            int[,] arr = new int[8,9];
            for(int i  = 0; i < arr.Length; i++)
            {
                for (int j = 0; j < arr.Length; j++)
                    arr[i, j] = (i + 2) * (j + 1);
                Console.WriteLine(arr[i, j], " ");
               
            }

        }
    }
}

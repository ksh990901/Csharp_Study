namespace GenericApp03
{
    class MyList<T>  where T : struct //한번에 작성이 가능하다. T는 값 형식이어야 합니다.
    {
    }
    class Cat<T> where T : class  // T는 참조 형식이어야 합니다.
    {
    }
    class Dog<T> where T : new() // T는 반드시 매개변수가 업는 생성자가 있어야 합니다. 
    {
    }
    internal class Program
    {
        static void Main(string[] args)
        {

        }
    }
}

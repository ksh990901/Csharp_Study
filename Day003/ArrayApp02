namespace ArrayApp02
{
    internal class Program
    {
        static void Main(string[] args)
        {
            int[] arr = new int[10];
            for (int i = 0, cnt =70; i < arr.Length; i++, cnt-=7)
           {
                arr[i] = cnt;
            }
            for(int i = 0; i < arr.Length;i++)
            {
                Console.Write(arr[i]);
            }
        }
    }
}
